	<queryString language="SQL">
		<![CDATA[SELECT 


project_project.date as "fecha_caducidad",
project_project.date_start as "fecha_inicio",
project_project.period_days as "delta_tiempo",
project_project.name as "nombre_proyecto",
sale_order.confirmation_date as "fecha_presup",
res_partner.name as "cliente",
res_company.name as "compania",

sale_order_line.name as "descripcion",
sale_order_line.price_unit as "precio_unit",
sale_order_line.product_uom_qty as "cantidad",
sale_order_line.price_subtotal as "subtotal",
res_currency.name as "moneda",


sale_order.amount_total as "total",
sale_order.client_order_ref as "ref_cliente",
sale_order.note as "observaciones",
rpu.name AS "Sale_comcercial",

iat.store_fname AS "Company_image",
icp.value AS "Filestore_path"


FROM sale_order
LEFT JOIN project_project on project_project.id = sale_order.project_id
LEFT JOIN res_partner on res_partner.id = sale_order.partner_id
LEFT JOIN res_company on project_project.company_id = res_company.id
LEFT JOIN sale_order_line on sale_order_line.order_id = sale_order.id
LEFT JOIN product_pricelist on product_pricelist.id = sale_order.pricelist_id
Left JOIN res_currency on res_currency.id = product_pricelist.currency_id

JOIN res_company rc ON rc.id = sale_order.company_id
JOIN res_partner rpc ON rpc.id = rc.partner_id

JOIN res_users ru
	ON sale_order.user_id = ru.id
JOIN res_partner rpu
	ON ru.partner_id = rpu.id

LEFT JOIN ir_attachment iat ON iat.res_id = rpc.id AND iat.res_model = 'res.partner' AND iat.res_field = 'image'
LEFT JOIN ir_config_parameter icp ON icp.key = 'filestore.path'


--WHERE sale_order.id = 37;

  WHERE $X{IN, sale_order.id, IDS};]]>

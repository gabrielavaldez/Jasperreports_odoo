	<parameter name="IDS" class="java.lang.Object"/>
	<queryString>
		<![CDATA[SELECT
sp.street AS calle,
sp.zip AS codigo_postal,
sp.city AS cuidad,
sp.package AS bultos,
sp.transport AS transporte,
sp.insurance_by AS asegurado,
sp.declared_value AS valor_declarado,
afp.name AS posicion_fiscal,
sp.vat AS cuit,
date(sp.scheduled_date) AS fecha,
sp.notes AS seniores,
rcs.name AS provincia,
sm.product_uom_qty AS cantidad,
pt.name AS descripcion,
sp.phone AS telefono


FROM stock_picking sp

LEFT JOIN res_country_state rcs

ON rcs.id = sp.res_contry_state_id

LEFT JOIN stock_move sm

ON sm.picking_id = sp.id

LEFT JOIN account_fiscal_position afp
ON afp.id = sp.fiscal_position_id

JOIN product_product AS pp
ON sm.product_id = pp.id

JOIN product_template AS pt
ON pp.product_tmpl_id = pt.id

WHERE $X{IN, sp.id, IDS}]]>
	</queryString>

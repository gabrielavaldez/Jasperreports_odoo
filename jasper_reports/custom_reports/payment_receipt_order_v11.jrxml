	<parameter name="IDS" class="java.lang.Object"/>
	<queryString>
		<![CDATA[SELECT
apo.id AS "Voucher_id",
apo.type AS "Voucher_type",
apo.amount_text AS "amount_text",

COALESCE(apo.number, 'Borrador') AS "Voucher_number",
COALESCE(TO_CHAR (apo.date, 'dd/mm/yyyy'), ' ') AS "Voucher_date",
apo.amount AS "Voucher_paid_amount",
apo.state AS "Voucher_state",
apo.narration AS "Voucher_internal_note",
rp.name AS "Partner_name",
COALESCE(rp.vat, ' ') AS "Partner_cuit",
rpc.name AS "Company_name",
rpc.vat AS "Company_cuit",
COALESCE(rpc.street, '') AS "Company_street",
COALESCE(rcsc.name, ' ') AS "Company_state",
COALESCE(rpc.city, ' ') AS "Company_city",
rpc.zip AS "Company_zip",
rpc.website AS "Company_website",
rpc.phone AS "Company_phone",
rpc.email AS "Company_email",
iat.store_fname AS "Company_image",
icp.value AS "Filestore_path"


FROM account_payment_order apo
JOIN res_company rc
	ON rc.id = apo.company_id
LEFT JOIN res_partner rp
	ON rp.id = apo.partner_id
JOIN res_partner rpc
	ON rc.partner_id = rpc.id
LEFT JOIN res_country_state rcsc
	ON rpc.state_id = rcsc.id
LEFT JOIN res_country rctyc
	ON rpc.country_id = rctyc.id
LEFT JOIN ir_attachment iat
	ON iat.res_id = rpc.id
	AND iat.res_model = 'res.partner'
	AND iat.res_field = 'image'
LEFT JOIN ir_config_parameter icp
	ON icp.key = 'filestore.path'

WHERE $X{IN, apo.id, IDS}

ORDER BY apo.number]]>
	</queryString>

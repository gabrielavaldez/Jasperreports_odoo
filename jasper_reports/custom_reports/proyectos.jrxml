	<queryString language="SQL">
		<![CDATA[select pp.date_start, pp.date as date_end, pp.id as project_id, pp.alcance, pp.code, pp.city, pp.partner_id, pp.certificacion, pp.establishment,
		pp.name, pp.description, rp_client.name as client_name, rp_user.name as jefe,

                /* image */
		iat.store_fname AS "Company_image",
		icp.value AS "Filestore_path"
		
		FROM project_project pp

                /* image */
		JOIN res_company rc ON rc.id = pp.company_id
		JOIN res_partner rpc ON rpc.id = rc.partner_id

		LEFT JOIN ir_attachment iat
			ON iat.res_id = rpc.id AND iat.res_model = 'res.partner' AND iat.res_field = 'image'
		LEFT JOIN ir_config_parameter icp 
			ON icp.key = 'filestore.path'

left join res_partner rp_client
	ON rp_client.id = pp.partner_id
left join res_users users
	ON users.id = pp.user_id
left join res_partner rp_user
	on rp_user.id = users.partner_id

	WHERE $X{IN, pp.id, IDS};]]>
	</queryString>

	<parameter name="IDS" class="java.lang.Object"/>
	<queryString>
		<![CDATA[SELECT
rpc.name AS "Company_name",
rpc.vat AS "Company_cuit",
COALESCE(rpc.street, '') AS "Company_street",
COALESCE(rcsc.name, ' ') AS "Company_state",
COALESCE(rpc.city, ' ') AS "Company_city",
rpc.zip AS "Company_zip",
rpc.website AS "Company_website",
rpc.phone AS "Company_phone",
rpc.email AS "Company_email",
iat.store_fname AS "Company_image",
icp.value AS "Filestore_path",

rp.name AS "Partner_name",
rp.vat AS "Partner_cuit",
COALESCE(rp.street, '') AS "Partner_street",
COALESCE(rcs.name, ' ') AS "Partner_state",
rp.city AS "Partner_city",
rp.zip AS "Partner_zip",
rp.phone AS "Partner_phone",
afp.name AS "Partner_fiscal_position",

pp.default_code AS "Line_code",
sol.name AS "Line_description",
sol.product_uom_qty AS "Line_quantity",
uom.name AS "Line_uom",
sol.price_unit AS "Line_price_unit",
sol.discount AS "Line_discount",
sol.price_subtotal AS "Line_subtotal",

so.name AS "Sale_number",
so.note AS "Sale_note",
so.confirmation_date AS "Sale_date",
COALESCE(pt.name, '') AS "Sale_project_type",
rpu.name AS "Sale_comcercial",
rpu.email AS "Sale_comercial_email",
so.amount_untaxed AS "Sale_subtotal",
so.amount_tax AS "Sale_vat",
so.amount_total AS "Sale_total",
project.name as "project_name",
currency.symbol as "moneda"



FROM sale_order so

JOIN sale_order_line sol
	ON so.id = sol.order_id
JOIN product_product pp
	ON sol.product_id = pp.id
JOIN uom_uom uom
	ON sol.product_uom = uom.id

JOIN res_users ru
	ON so.user_id = ru.id
JOIN res_partner rpu
	ON ru.partner_id = rpu.id
LEFT JOIN project_type pt
	ON so.project_type_id = pt.id

JOIN res_partner rp
	ON so.partner_id = rp.id
LEFT JOIN res_country_state rcs
	ON rp.state_id = rcs.id
LEFT JOIN res_country rcty
	ON rp.country_id = rcty.id
LEFT JOIN ir_property ip
	ON ip.name ~ 'property_account_position'
    AND ip.res_id=CONCAT('res.partner,',rp.id)
LEFT JOIN account_fiscal_position afp
	ON split_part(ip.value_reference, ',', 2)::int=afp.id
LEFT JOIN res_document_type rdt
	ON rdt.id = rp.document_type_id


JOIN res_company rc
	ON so.company_id = rc.id
JOIN res_partner rpc
	ON rc.partner_id = rpc.id
LEFT JOIN res_country_state rcsc
	ON rpc.state_id = rcsc.id
LEFT JOIN res_country rctyc
	ON rpc.country_id = rctyc.id
LEFT JOIN ir_attachment iat
	ON iat.res_id = rpc.id
	AND iat.res_model = 'res.partner'
	AND iat.res_field = 'image'
LEFT JOIN ir_config_parameter icp
	ON icp.key = 'filestore.path'
LEFT JOIN project_project project
	ON project.id = so.project_id
LEFT JOIN product_pricelist ppr
	ON so.pricelist_id = ppr.id
LEFT JOIN res_currency currency
	ON ppr.currency_id = currency.id

WHERE $X{IN, so.id, IDS}]]>
	</queryString>

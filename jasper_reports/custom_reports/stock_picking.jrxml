<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="stock_picking" language="groovy" pageWidth="595" pageHeight="842" columnWidth="555" leftMargin="20" rightMargin="20" topMargin="20" bottomMargin="20">
	<property name="ireport.zoom" value="1.5"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<queryString>
		<![CDATA[SELECT
sp.id AS picking_id,
sp.date_done::date AS picking_date,
sm.name AS product_name,
sm.product_uom_qty AS product_quantity

FROM stock_picking sp
JOIN stock_move sm
	ON sp.id = sm.picking_id

WHERE sp.id = 303]]>
	</queryString>
	<field name="picking_id" class="java.lang.Integer"/>
	<field name="picking_date" class="java.sql.Date"/>
	<field name="product_name" class="java.lang.String"/>
	<field name="product_quantity" class="java.math.BigDecimal"/>

project_project.date_start as "fecha_inicio",
project_project.period_days as "delta_tiempo",
sale_order.validity_date as "fecha_presup",
res_partner.name as "cliente",
res_company.name as "compania",

sale_order_line.name as "descripcion",
sale_order_line.price_unit as "precio_unit",
sale_order_line.product_uom_qty as "cantidad",
sale_order_line.price_subtotal as "subtotal",


sale_order.amount_total as "total",
sale_order.note as "observaciones",

iat.store_fname AS "Company_image",
icp.value AS "Filestore_path"


FROM sale_order
LEFT JOIN project_project on project_project.id = sale_order.project_id

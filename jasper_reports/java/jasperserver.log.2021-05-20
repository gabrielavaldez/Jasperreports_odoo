LEFT JOIN project_project on project_project.id = sale_order.project_id
LEFT JOIN res_partner on res_partner.id = sale_order.partner_id
LEFT JOIN res_company on project_project.company_id = res_company.id
LEFT JOIN sale_order_line on sale_order_line.order_id = sale_order.id

JOIN res_company rc ON rc.id = sale_order.company_id
JOIN res_partner rpc ON rpc.id = rc.partner_id

LEFT JOIN ir_attachment iat ON iat.res_id = rpc.id AND iat.res_model = 'res.partner' AND iat.res_field = 'image'
LEFT JOIN ir_config_parameter icp ON icp.key = 'filestore.path'


WHERE sale_order.id = 37;

 -- WHERE sale_order.id IN (?);


